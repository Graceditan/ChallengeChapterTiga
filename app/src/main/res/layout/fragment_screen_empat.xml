<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    android:layout_gravity="center_horizontal"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ScreenEmpatFragment">

    <EditText
        android:id="@+id/et_usia"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:hint="Masukkan Usia"
        android:textSize="35dp"
        android:layout_marginTop="100dp"
        android:background="@drawable/kotaknama">
    </EditText>

    <EditText
        android:id="@+id/et_alamat"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:hint="Masukkan Alamat"
        android:textSize="35dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/kotaknama">
    </EditText>

    <EditText
        android:id="@+id/et_pekerjaan"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:hint="Masukkan Pekerjaan"
        android:textSize="33dp"
        android:background="@drawable/kotaknama">
    </EditText>


    <Button
        android:id="@+id/btn_backtoscreentiga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="Back To Screen 3"
        android:layout_marginTop="30dp">

    </Button>






</LinearLayout>